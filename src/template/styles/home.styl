body{
    font-size: 12px;
    background-color: #fff;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.page-home{
    text-align: center;
    padding-top: 40px;
    .icon{
        width: 100px;
        height: 100px;
        margin: 0 auto;
    }
    
    .icon-run{
        background-image:url('../images/home/logo.png');
        background-size: 100px 100px;
    }
    .title{
        margin: 0 auto;
        font-size: 14px;
        span{
            display: inline-block;
            padding: 4px 10px;
            background: #333;
            color: #fff;
            border-radius: 15px;
        }
    }
    .content{
        color: #666;
        padding: 10px;
    }
}